### appendPostposition

#!fsharp

let appendPostposition (s: string) (withConsonant: string) (withoutConsonant: string) =
    let lastTail = lastConsonant s
    if int lastTail <> 0 then
        if withConsonant = "으로" && int lastTail = TailL then
            s + withoutConsonant
        else
            s + withConsonant
    else
        s + withoutConsonant

