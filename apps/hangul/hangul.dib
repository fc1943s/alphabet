### leadIdx

#!fsharp

let leadIdx (r: int) =
    let i = r - leadBase
    if i >= 0 && i <= maxLeadIdx then Some i
    else None

#!markdown

### medialIdx

#!fsharp

let medialIdx (r: int) =
    let i = r - medialBase
    if i >= 0 && i <= maxMedialIdx then Some i
    else None

#!markdown

### tailIdx

#!fsharp

let tailIdx (r: int) =
    if r = 0 then Some 0
    else
        let i = r - tailBase
        if i >= 0 && i <= maxTailIdx then Some (i + 1)
        else None

