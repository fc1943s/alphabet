### toCp949

#!fsharp

let toCp949 (s: string) =
    s
    |> Seq.collect (fun c ->
        let ucs2code = uint16 c
        if ucs2code < 0x80us then
            [| byte ucs2code |]
        else
            let compareFunc (item: LookupItem) = int item.ucs2 - int ucs2code
            match binarySearch cp949ToTable compareFunc with
            | Some idx ->
                let cp949code = cp949ToTable.[idx].cp949
                [| byte (cp949code >>> 8); byte (cp949code &&& 0xFF) |]
            | None ->
                [| byte 0x3F |] // '?'
    )
    |> Seq.toArray

