#!markdown

### binarySearch

#!fsharp

let binarySearch (table: 'T[]) (compare: 'T -> int) =
    let rec search low high =
        if low > high then
            None
        else
            let mid = low + (high - low) / 2
            let c = compare table.[mid]
            if c < 0 then
                search (mid + 1) high
            elif c > 0 then
                search low (mid - 1)
            else
                Some mid
    search 0 (table.Length - 1)

#!markdown

### fromCp949

#!fsharp

let fromCp949 (data: byte[]) =
    let rec processBytes bytes =
        match bytes with
        | [] -> []
        | b1 :: rest when b1 < 0x80uy ->
            char b1 :: processBytes rest
        | b1 :: b2 :: rest ->
            let cp949code = (uint16 b1 <<< 8) ||| uint16 b2
            let compareFunc (item: LookupItem) = int item.cp949 - int cp949code
            let charToYield =
                match binarySearch cp949FromTable compareFunc with
                | Some idx -> char cp949FromTable.[idx].ucs2
                | None -> char 0xFFFD
            charToYield :: processBytes rest
        | _ -> [char 0xFFFD]
    data |> Array.toList |> processBytes |> List.toArray |> System.String

